vector< vector<string> >GESTION_ACCESS::lire_demande()
{
  vector< vector<string> > lecture;
  vector<string> valeur;



		  for(int i=0;i<8;i++)
		  {
			  valeur.push_back("ew-aa-202");
		  }
		  lecture[0].push_back("proprietaire");
		  lecture[0].push_back("plaque");
		  lecture[0].push_back("horodatage");
		  lecture.push_back(valeur);


    return lecture;

}

