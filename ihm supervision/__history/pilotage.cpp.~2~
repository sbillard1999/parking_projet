//---------------------------------------------------------------------------

#pragma hdrstop

#include "pilotage.h"
//---------------------------------------------------------------------------
string pilotage::ouverture()
{
	this->etat="ouvert";
	return etat;
}
string pilotage::fermeture()
{
	this->etat="fermé";
	return etat;
}
pilotage::pilotage()
{
  this->etat="fermé";
}

#pragma package(smart_init)
